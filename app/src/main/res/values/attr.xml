<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--图标-->
    <attr name="icon" format="reference"></attr>
    <!--渐变颜色-->
    <attr name="color" format="color"></attr>
    <!--文本信息-->
    <attr name="text" format="string"></attr>
    <!--文字大小-->
    <attr name="text_size" format="dimension"></attr>

    <declare-styleable name="ChangeIcon">
        <attr name="icon"></attr>
        <attr name="color"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
    </declare-styleable>

    <attr name="textHighLight" format="color" />
    <attr name="textNormal" format="color" />
    <attr name="textWeak" format="color" />
    <attr name="appBackGround" format="color" />
    <!-- 自定义验证码输入框-->
    <declare-styleable name="VerificationCodeView">
        <!--输入框的数量-->
        <attr name="icv_et_number" format="integer" />
        <!--输入框的宽度-->
        <attr name="icv_et_width" format="dimension|reference" />
        <!--输入框之间的分割线-->
        <attr name="icv_et_divider_drawable" format="reference" />
        <!--输入框文字颜色-->
        <attr name="icv_et_text_color" format="color|reference" />
        <!--输入框文字大小-->
        <attr name="icv_et_text_size" format="dimension|reference" />
        <!--输入框获取焦点时边框-->
        <attr name="icv_et_bg_focus" format="reference" />
        <!--输入框没有焦点时边框-->
        <attr name="icv_et_bg_normal" format="reference" />
    </declare-styleable>

</resources>